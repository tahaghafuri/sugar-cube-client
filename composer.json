{
  "name": "tahaghafuri/sugar-cube-client",
  "description": "A sweet Gitea API client for PHP",
  "homepage": "https://github.com/tahaghafuri/sugar-cube-client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {"email": "tahaghafuri@gmail.com", "name": "Taha Amin Ghafuri", "homepage": "https://tahaghafuri.ir"}
  ],
  "autoload": {
    "psr-4": {"Gitea\\": "src/"}
  },
  "keywords": [
    "api",
    "client",
    "git",
    "gitea",
    "sugar",
    "scm",
    "vcs"
  ],
  "require": {
    "php": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "myclabs/php-enum": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "*",
    "phpunit/phpunit": "*"
  },
  "support": {
    "docs": "https://github.com/tahaghafuri/sugar-cube-client",
    "issues": "https://github.com/tahaghafuri/sugar-cube-client/issues"
  }
}
